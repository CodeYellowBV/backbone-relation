{
  "name": "backbone-relation",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "backbone": "1.2.1",
    "lodash": "^3.10.0"
  },
  "devDependencies": {
    "babel": "5.6.14",
    "babel-core": "5.6.15",
    "babel-loader": "^5.3.1",
    "codeyellow-style": "git+https://bitbucket.org/codeyellow/codeyellow-style.git#v0.1.2",
    "eslint": "1.1.0"
  },
  "scripts": {
    "pretest": "npm run -s build-js && npm run -s lint-js",
    "test": "echo 'Running backbone tests...'; ./backbone/node_modules/.bin/karma start",
