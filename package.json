{
  "name": "backbone-relation",
  "version": "0.0.0",
  "private": true,
  "main": "dist/backbone-relation.js",
  "dependencies": {
    "backbone": "1.0.0 - 1.2.3",
    "underscore": "1.4.4 - 1.8.3"
  },
  "devDependencies": {
    "webpack": "^1.12.2",
    "babel-core": "5.6.15",
    "babel-loader": "^5.3.1",
    "codeyellow-style": "git+https://bitbucket.org/codeyellow/codeyellow-style.git#v0.1.2",
    "eslint": "1.6.0"
  },
  "scripts": {
    "build": "webpack --progress --bail --colors",
    "pretest": "npm run -s build && npm run -s lint-js",
    "test": "echo 'Running backbone tests...'; ./backbone/node_modules/.bin/karma start",
    "lint-js": "eslint index.js",
    "deps": "git submodule update --init --recursive && npm install && cd backbone; npm install"
  }
}
